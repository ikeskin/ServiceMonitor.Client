{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "ServiceMonitor.Client": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ServiceMonitor": {
    "DashboardUrl": "http://localhost:5192",
    "ApiKey": "sm_live_your_api_key_here",
    "ServiceName": "web-api-sample",
    "Environment": "development",
    "Version": "1.0.0",
    "HeartbeatInterval": "00:00:30",
    "EnableMetrics": true,
    "EnableLogging": true,
    "RetryAttempts": 3
  }
}
